{
  "name": "react-input-mask-updated",
  "description": "Masked input component for React (fork by Reflexions)",
  "version": "3.0.4",
  "homepage": "https://github.com/reflexions/react-input-mask",
  "license": "MIT",
  "author": "Reflexions <contact@reflexions.co>",
  "keywords": [
    "react",
    "input",
    "mask",
    "masked",
    "react-component"
  ],
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/plugin-transform-proto-to-assign": "^7.16.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-dev-expression": "^0.2.3",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.3.4",
    "console-polyfill": "^0.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "lint-staged": "^11.2.6",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1",
    "puppeteer": "^11.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^2.59.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "main": "index.js",
  "files": [
    "lib",
    "dist"
  ],
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "cross-env BABEL_ENV=rollup rollup -c",
    "start": "cross-env BABEL_ENV=test NODE_ENV=development webpack-dev-server",
    "prepare": "npm run lint && npm test && npm run clean && npm run build",
    "lint": "eslint ./src ./tests ./*.js",
    "lint-fix": "eslint --fix ./src ./tests ./*.js",
    "test": "npm run build && npm run test:input && npm run test:server-render && npm run test:build",
    "test:input": "cross-env NODE_ENV=test BABEL_ENV=test karma start",
    "test:server-render": "cross-env BABEL_ENV=test mocha --require @babel/register ./tests/server-render",
    "test:build": "cross-env BABEL_ENV=test mocha --require @babel/register ./tests/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reflexions/react-input-mask.git"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "prop-types": "^15.7.2",
    "warning": "^4.0.3"
  }
}
